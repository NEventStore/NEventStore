        <tags>events, event sourcing, cqrs, storage, persistence, database</tags>
        <releaseNotes></releaseNotes>

        <dependencies>
            <dependency id="NEventStore" version="[$version$]"/>
        </dependencies>
    </metadata>

    <files>
        <file src="publish-net40\NEventStore.Serialization.Json\bin\**" target="lib\net40"/>
        <file src="src\NEventStore.Serialization.Json\**\*.cs" target="src\NEventStore.Serialization.Json"/>
    </files>
