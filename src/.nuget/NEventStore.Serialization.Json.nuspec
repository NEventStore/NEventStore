<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>NEventStore.Serialization.Json</id>
        <version>8.0</version>
        <title>NEventStore Json Serializers</title>
        <authors>NEventStore Dev Team</authors>
        <owners>NEventStore Dev Team</owners>
        <projectUrl>http://neventstore.org</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This package contains Json and Bson serializers for NEventStore library</description>
